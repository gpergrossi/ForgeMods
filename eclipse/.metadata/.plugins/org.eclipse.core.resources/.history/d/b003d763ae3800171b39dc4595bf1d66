package dev.mortus.aerogen;

import net.minecraft.world.DimensionType;
import net.minecraft.world.WorldProvider;
import net.minecraftforge.common.DimensionManager;

public class WorldProviderSky extends WorldProvider {

	private static final int dimensionID = 2;
	private static DimensionType dimensionType; 
	
    public static DimensionType getDimensionType() {
    	if (dimensionType == null) {
    		dimensionType = DimensionType.register("The Sky", "_sky", dimensionID, WorldProviderSky.class, false);
    	}
    	
    	DimensionType result = null;
    	try {
    		DimensionType.getById(dimensionID);
    	}
    	
    	return dimensionType;
    }
	
	@Override
	public DimensionType getDimensionType() {
		return Dimension
	}

}
