package dev.mortus.test.gui;

import java.awt.Graphics2D;

import dev.mortus.chunks.Chunk;
import dev.mortus.chunks.ChunkManager;

public abstract class View2DChunk<T extends Chunk<?>> extends Chunk<T> {

	public View2DChunk(ChunkManager<T> manager, int chunkX, int chunkY) {
		super(manager, chunkX, chunkY);
	}

	public abstract void draw(Graphics2D g);
	
}
