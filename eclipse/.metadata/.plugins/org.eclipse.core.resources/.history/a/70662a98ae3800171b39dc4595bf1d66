package dev.mortus.aerogen;

import net.minecraft.world.DimensionType;
import net.minecraft.world.WorldProvider;

public class WorldProviderSky extends WorldProvider {

	private static final int dimensionID = 2;
	DimensionType dimensionType; 
	
	@Override
	public DimensionType getDimensionType() {
    	DimensionType result = null;
    	try {
    		result = DimensionType.getById(dimensionID);
    	} catch (IllegalArgumentException e) {
    		result = DimensionType.register("The Sky", "_sky", dimensionID, WorldProviderSky.class, false);
    	}
    	
    	return result;
	}

}
